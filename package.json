{
  "name": "int20h-2019",
  "version": "1.0.0",
  "description": "",
  "main": "src/host/index.ts",
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./build",
    "start": "node build/index.js",
    "heroku-postbuild": "npm run build:prod",
    "build:prod": "npm run clean && tsc --p ./src/host/ && webpack --config ./src/client/webpack.config.js --mode production",
    "build": "npm run clean && tsc --p ./src/host/ &&  webpack --config ./src/client/webpack.config.js --mode development --progress",
    "dev": "npm run build && npm-run-all --parallel watch:server watch:build start:server",
    "watch:build": "webpack --config ./src/client/webpack.config.js --watch --mode development",
    "watch:server": "tsc --p ./src/host/ --watch",
    "start:server": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/101gram/INT20H-2019.git"
  },
