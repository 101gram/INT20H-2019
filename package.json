  "bugs": {
    "url": "https://github.com/101gram/INT20H-2019/issues"
  },
  "homepage": "https://github.com/101gram/INT20H-2019#readme",
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/node": "10.12.21",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^1.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "react-redux": "^5.1.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.3.1",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
